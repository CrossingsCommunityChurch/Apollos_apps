{
    "family": "ccc-apollos-api-dev",
    "executionRoleArn": "arn:aws:iam::630260715093:role/ecsTaskExecutionRole",
    "networkMode": "bridge",
    "containerDefinitions": [
      {
        "name": "ccc_graphapi",
        "image": "630260715093.dkr.ecr.us-east-1.amazonaws.com/ccc_graphapi:latest",
        "links": [],
        "cpu": 10,
        "memory": 512,
        "portMappings": [
          {
            "containerPort": 4000,
            "hostPort": 0,
            "protocol": "tcp"
          }
        ],
        "essential": true,
        "entryPoint": [],
